{
  "name": "@simora/teach",
  "version": "0.0.1",
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0",
  "scripts": {
    "dev:next": "next dev --turbopack",
    "dev": "run-p dev:*",
    "build": "next build --debug --no-lint --no-mangling ",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage node_modules && pnpm store prune",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:before-all": "tsc --noEmit --pretty  --project tsconfig.typecheck.json",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "dotenv -c production -- drizzle-kit migrate",
    "db:reset": "dotenv -c production -- tsx lib/resetDatabase.ts && rm -rf migrations",
    "db:seed": "dotenv -c production -- tsx lib/seed.ts",
    "db:studio": "dotenv -c production -- drizzle-kit studio",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6006 --silent",
    "serve-storybook": "run-s storybook:*",
    "test-storybook:ci": "start-server-and-test serve-storybook http://127.0.0.1:6006 test-storybook",
    "prepare": "husky"
  },
